{
  "name": "javascript-starter-kit",
  "version": "1.0.0",
  "description": "javascript starter kit",
  "main": "index.js",
  "author": "Anton Zdanov <azdanov@protonmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {},
  "scripts": {
    "security-check": "nsp check",
    "open:src": "babel-node buildScripts/srcServer.js",
    "generate-mock-data": "babel-node buildScripts/generateMockData.js",
    "prestart-mockapi": "yarn generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "predev": "yarn clean:dist",
    "dev": "webpack --mode development --config webpack.config.dev.babel.js",
    "watch": "yarn run dev --watch",
    "serve": "webpack-dev-server --mode development --open",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "yarn lint --watch",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "clean:dist": "rimraf dist",
    "prebuild": "npm-run-all clean:dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "dependencies": {
    "raven-js": "^3.23.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/register": "7.0.0-beta.40",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8",
    "babel-jest": "^22.4.1",
    "babel-loader": "^8.0.0-beta.0",
    "chalk": "2.3.2",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.2",
    "cross-env": "5.1.3",
    "css-loader": "0.28.10",
    "eslint": "4.x",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^4.0.2",
    "eslint-watch": "^3.1.3",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.4.2",
    "jsdom": "11.6.2",
    "json-schema-faker": "0.5.0-rc13",
    "json-server": "0.12.1",
    "localtunnel": "1.8.3",
    "nock": "9.2.3",
    "node-sass": "^4.7.2",
    "npm-run-all": "4.1.2",
    "nsp": "^3.2.1",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "prettier": "^1.11.1",
    "rimraf": "2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "0.20.2",
    "uglifyjs-webpack-plugin": "latest",
    "webpack": "4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-middleware": "3.0.1",
    "webpack-hot-middleware": "2.21.2",
    "webpack-md5-hash": "0.0.6",
    "webpack-subresource-integrity": "^1.1.0-rc.2"
  }
}
